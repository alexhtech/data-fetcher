{
    "name": "data-fetcher",
    "version": "2.0.0-beta.2",
    "description": "It is just a wrapper around fetch",
    "repository": "git@github.com:alexolefirenko/data-fetcher.git",
    "author": "Alex <olefirenk@gmail.com>",
    "license": "MIT",
    "main": "./commonjs/index.js",
    "module": "./es6/index.js",
    "jsnext:main": "./es6/index.js",
    "scripts": {
        "build:es6": "cross-env NODE_ENV=es6 babel src --out-dir ./es6",
        "build:commonjs": "cross-env NODE_ENV=commonjs babel src --out-dir ./commonjs",
        "build": "npm run clean && npm run build:es6 && npm run build:commonjs",
        "clean": "rm -rf ./commonjs ./es6",
        "test": "jest",
        "deploy": "yarn test && yarn build && yarn publish"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "cross-env": "^5.1.1"
    },
    "dependencies": {
        "is-browser": "^2.0.1",
        "jest": "^22.0.6",
        "qs": "6.5.1"
    }
}
